
public class Principal {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		for(int i = 150; i < 301; i++){
			System.out.println(i);
		}
		
		int soma =0;
		int i = 1;
		while(i < 1001){
			soma += i;
			i++;
		}
		System.out.println("Soma 1 ate 1000 :"+soma);
	}

}
