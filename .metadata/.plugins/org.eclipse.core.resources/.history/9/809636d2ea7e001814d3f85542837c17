package View;

import Model.*;

public class Principal {
	public static void main(String[] args) {
	
		Pessoa julio = new Pessoa("julio",11);
		julio.FazAniversario();
		System.out.println(julio);
		
		Porta portaCasa = new Porta();
		portaCasa.abre();
		System.out.println(portaCasa.isAberta());
		
	}
}
