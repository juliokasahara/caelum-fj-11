
public class Principal {

	public static void main(String[] args) {
		

			System.out.println(CalculaFibonacci(3));


	}

	public static int CalculaFibonacci(int n) {
		   if (n == 0)
		      return 0;
		   if (n == 1)
		      return 1;
		   return CalculaFibonacci(n-1) + CalculaFibonacci(n-2);
		}
}


