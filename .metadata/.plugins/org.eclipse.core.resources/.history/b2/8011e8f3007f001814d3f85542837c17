
public class Principal {

	public static void main(String[] args) {
		
		for(int i = 0 ; i < 5 ; i++)
			System.out.println(CalculaFibonacci(i));
	}

	public static int CalculaFibonacci(int n) {
		   if (n == 0)
		      return 0;
		   if (n == 1)
		      return 1;
		   return CalculaFibonacci(n-1) + CalculaFibonacci(n-2);
		}
}


