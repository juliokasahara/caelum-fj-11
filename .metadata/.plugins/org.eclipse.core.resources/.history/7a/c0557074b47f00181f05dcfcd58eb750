package Model;

public class RelatorioFuncionarios {

	public void Relatorio(EmpregadoDaFaculdade empregadoDaFaculdade) {
		System.out.println("---------Relatorio----------------");
		System.out.println(empregadoDaFaculdade.getGastos());
		System.out.println(empregadoDaFaculdade.getInfo());
		System.out.println("-----------------------------------");
	}
}
